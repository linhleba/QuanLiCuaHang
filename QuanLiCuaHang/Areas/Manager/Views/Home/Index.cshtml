                        { "elapsed": "VIII", "value": 26 },
                        { "elapsed": "IX", "value": 12 },
                        { "elapsed": "X", "value": 19 }
                    ];
                    Morris.Line({
                        element: 'graph9',
                        data: day_data,
                        xkey: 'elapsed',
                        ykeys: ['value'],
                        labels: ['Tổng cộng'],
                        parseTime: false
                    });
                </script>

            </div>
        </div>
